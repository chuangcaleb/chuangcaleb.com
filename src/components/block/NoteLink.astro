---
import type { SuperNote } from "lib/utils/types";
import AccentA from "../styled/monom/AccentA.astro";

interface Props {
  note: SuperNote;
  variant?: "accent";
}

const { note, variant } = Astro.props;
const Tag = variant === "accent" ? AccentA : "a";
---

<Tag href={note.route}>
  {note.data?.emojip && `${note.data?.emojip} `}
  {note.name}
</Tag>
