{
  "name": "chuangcaleb.com",
  "type": "module",
  "version": "2.0.0",
  "scripts": {
    "astro": "astro",
    "format:check": "pnpm xo -- --list-different .",
    "format:fix": "pnpm xo --write .",
    "test": "xo",
    "b2:download": "node lib/b2/index.mjs && tsx lib/chuangcaleb/generate-redirects.ts",
    "setup-external": "pnpm b2:download",
    "dev": "pnpm setup-external && astro dev",
    "dev:nosetup": "astro dev",
    "build": "pnpm setup-external && astro check && astro build && pnpm pagefind",
    "build:nosetup": "astro check && astro build",
    "pagefind": "pagefind --site dist --output-subdir _pagefind",
    "pagefind:dev": "pnpm pagefind && cp -r dist/_pagefind public/",
    "preview:nobuild": "astro preview",
    "preview:nosetup": "pnpm build:nosetup && astro preview",
    "preview": "pnpm build && astro preview"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.7.0",
    "@iconify-json/lucide": "^1.2.90",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/simple-icons": "^1.2.70",
    "@portaljs/remark-wiki-link": "^1.2.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "astro": "^5.17.2",
    "astro-compressor": "^1.2.0",
    "astro-icon": "^1.1.5",
    "astro-pagefind": "^1.8.5",
    "astro-purgecss": "^5.5.0",
    "astro-robots-txt": "^1.0.0",
    "backblaze-b2": "^1.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "decompress": "^4.2.1",
    "dotenv": "^17.3.1",
    "fs-extra": "^11.3.3",
    "github-slugger": "^2.0.0",
    "google-auth-library": "^10.5.0",
    "google-spreadsheet": "^5.2.0",
    "googleapis": "^171.4.0",
    "js-yaml": "^4.1.1",
    "mdast-util-to-string": "^4.0.0",
    "pagefind": "^1.4.0",
    "postcss-custom-media": "^11.0.6",
    "postcss-preset-env": "^10.6.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rehype-external-links": "^3.0.0",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "utopia-core-scss": "^1.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/hast": "^3.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/unist": "^3.0.3",
    "astro-og": "^0.3.2",
    "autoprefixer": "^10.4.24",
    "postcss": "^8.5.6",
    "sass": "^1.97.3",
    "tsx": "^4.21.0",
    "unist-util-visit": "^5.1.0",
    "xo": "^1.2.3"
  }
}
