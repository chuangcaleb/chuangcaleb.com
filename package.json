{
  "name": "chuangcaleb.com",
  "type": "module",
  "version": "2.0.0",
  "scripts": {
    "astro": "astro",
    "format:check": "pnpm xo -- --list-different .",
    "format:fix": "pnpm xo --write .",
    "test": "xo",
    "b2:download": "node lib/b2/index.mjs && tsx lib/chuangcaleb/generate-redirects.ts",
    "setup-external": "pnpm b2:download",
    "dev": "pnpm setup-external && astro dev",
    "dev:nosetup": "astro dev",
    "build": "pnpm setup-external && astro check && astro build && pnpm pagefind",
    "build:nosetup": "astro check && astro build",
    "pagefind": "pagefind --site dist --output-subdir _pagefind",
    "pagefind:dev": "pnpm pagefind && cp -r dist/_pagefind public/",
    "preview:nobuild": "astro preview",
    "preview:nosetup": "pnpm build:nosetup && astro preview",
    "preview": "pnpm build && astro preview"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.4.1",
    "@portaljs/remark-wiki-link": "^1.2.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "astro": "^5.11.0",
    "astro-compressor": "^1.0.0",
    "astro-iconify": "^1.2.0",
    "astro-pagefind": "^1.8.3",
    "astro-purgecss": "^5.3.0",
    "astro-robots-txt": "^1.0.0",
    "backblaze-b2": "^1.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "decompress": "^4.2.1",
    "dotenv": "^17.0.1",
    "fs-extra": "^11.3.0",
    "github-slugger": "^2.0.0",
    "google-auth-library": "^9.15.1",
    "google-spreadsheet": "^4.1.4",
    "googleapis": "^148.0.0",
    "js-yaml": "^4.1.0",
    "mdast-util-to-string": "^4.0.0",
    "pagefind": "^1.3.0",
    "postcss-custom-media": "^11.0.6",
    "postcss-preset-env": "^10.2.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.2",
    "typescript": "^5.8.3",
    "unified": "^11.0.5",
    "utopia-core-scss": "^1.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/hast": "^3.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/unist": "^3.0.3",
    "astro-og": "^0.3.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "sass": "^1.89.2",
    "tsx": "^4.20.3",
    "unist-util-visit": "^5.0.0",
    "xo": "^1.1.1"
  }
}
